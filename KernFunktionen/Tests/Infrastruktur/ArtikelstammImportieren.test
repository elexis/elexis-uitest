get-view Patienten | get-editbox | set-focus
get-button "Artikelöffnen" | click
with [get-view Artikel] {
    get-tab-folder | get-tab-item Artikelstamm | click
    get-editbox -after [get-label "Artikel oder Wirkstoff"] | set-focus
}
get-button "Menü anzeigen" | click
get-view Artikel | get-menu -path "Import..." | click
with [get-window Datenimport | get-editbox -after [get-label "Bitte Datei angeben:"]] {
    click-text 47
    key-type "M1+v"
    set-text "/home/niklaus/elexis/artikelstamm_first_v5.xml"
    key-type "M1+v"
}
exec-with-options {
    get-window Datenimport | get-button OK | click
    get-window "Fehler aufgetreten" | get-button OK | click
} -allowStatusDialog
