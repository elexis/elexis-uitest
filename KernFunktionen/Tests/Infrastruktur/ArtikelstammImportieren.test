Content-Type: text/ecl
Entry-Name: .content

get-view [ t view_patients ] | get-editbox | set-focus
get-button [ t button_open_articles ] | click
with [get-view [ t artikel ] ] {
    get-tab-folder | get-tab-item Artikelstamm | click
    get-editbox -after [get-label "Artikel oder Wirkstoff"] | set-focus
}
get-button [ t show_menu ] | click
get-view [ t artikel ] | get-menu -path "Import.*" | click
t view_data_select_file  | log
with [get-window [ t view_data_import ]| get-editbox -after [get-label [ t view_data_select_file ] ] ] {
    click-text
    key-type "M1+v"
    set-text "/"
    key-type "M1+v"
}

exec-with-options {
    get-window [ t view_data_import ]| get-button OK | click
    get-window "Fehler aufgetreten" | get-button OK | click
} -allowStatusDialog
