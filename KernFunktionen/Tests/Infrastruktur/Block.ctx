proc "SelectBlockInBlocks" [ val blockName "arzt_Berichte \\(KVG\\)" ] {
	open-window-by-name Codes
	with [ get-view Codes ]	{
		get-tab-folder | get-tab-item "Blöcke" | click
		try {
			get-tree | select $blockName
			bool true
		} -catch {
			concat "Failed SelectBlockInBlocks" $blockName | log
			bool false
		}
	}
}

proc "DeleteBlockInBlocks" [ val blockName "arzt_Berichte \\(KVG\\)" ] {
	open-window-by-name Codes
	get-view Codes | get-tree | select $blockName | get-menu -path "Block löschen" | click
	concat "SelectBlockInBlocks for " $blockName " returns: " [ SelectBlockInBlocks ] | log
	SelectBlockInBlocks | not | assert-true
}
