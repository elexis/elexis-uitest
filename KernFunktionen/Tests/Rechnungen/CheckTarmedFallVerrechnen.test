proc "CheckTarmedXml"  [ val invoiceId ] [ val searchFor ] {
	let [ val xmlFile [ concat $pdfDir $invoiceId ".xml" ] ] {
		concat "CheckTarmedXml " $invoiceId " looking for '" $searchFor "'" | log
		FileContainsString $xmlFile -searchFor $searchFor | assert-true
	}
}

proc "CheckTarmedFallVerrechnen"   [ val invoiceId 1 ] {
	CheckTarmedXml $invoiceId LOSARTAN
	CheckTarmedXml $invoiceId Metoprolol
	CheckTarmedXml $invoiceId "Klinische Geruchsprüfung, beidseitig"
	CheckTarmedXml $invoiceId "Konsultation, erste 5 Min. (Grundkonsultation)"
	CheckTarmedXml $invoiceId "Zuschlag für hausärztliche Leistungen in der Arztpraxis"
	CheckTarmedXml $invoiceId "Konsultation bei Personen über 6 Jahren und unter 75 Jahren mit einem erhöhten Behandlungsbedarf, jede weiteren 5 Min."
	CheckTarmedXml $invoiceId "Konsultation, letzte 5 Min. (Konsultationszuschlag)"
	CheckTarmedXml $invoiceId "Ultraschalluntersuchung ganzes Abdomen (Oberbauch und Unterbauch)"
	concat "CheckTarmedFallVerrechnen " $invoiceId " erfolgreich" | log
}

CheckTarmedFallVerrechnen
