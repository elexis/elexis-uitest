Content-Type: text/ecl
Entry-Name: .content

get-preferences-menu | click
with [get-window Benutzervorgaben] {
    get-tree | select "Gruppen und Rechte/Benutzerverwaltung"
    get-button -after [get-label Benutzerverwaltung] -index 3 | click
}
with [get-window "Benutzernamen festlegen"] {
    get-editbox 
        -after [get-label "Benutzernamen festlegen - dieser kann nicht mehr geändert, sowie zukünftig anderweitig verwendet werden."] 
        | set-text user
    get-button OK | click
}
with [get-window Benutzervorgaben] {
    get-table | select user
    with [get-group Verrechnung] {
        get-group Rollenzuordnung | get-table | get-item -path fhir | check
        get-group "tätig für" | get-table | get-item -path "Bond James" | check
    }
    get-window "No contact assigned" | get-button OK | click
    get-group Verrechnung | get-link "Nicht gesetzt <a>ändern</a>" | click-link
    with [get-window "Kontakt auswählen"] {
        get-table | select "Bond James - Bond James \\(m\\), , Dr. med. \\(\\)"
        get-button OK | click
    }
    get-button "Anwenden und Schließen" | click
}
get-preferences-menu | click
