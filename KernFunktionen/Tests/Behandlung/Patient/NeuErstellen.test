Patient über View Patienten, Button add neu erstellen
------=_.description-216f885c-d591-38ce-8ea2-e4f8cb4d6ffa--
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view Patienten | get-button "Neuen Patienteneintrag erstellen" | click
with [get-window "Patient erfassen"] {
    with [get-editbox -after [get-label Name]] {
        set-text $currentUniqPatient
        key-type "TRAVERSE_TAB_NEXT"
    }
    with [get-editbox -after [get-label Vorname]] {
        set-text Vorname
        key-type "TRAVERSE_TAB_NEXT"
    }
    get-combo -after [get-label Geschlecht] | key-type "TRAVERSE_TAB_NEXT"
    with [get-editbox -after [get-label Geburtsdatum]] {
        set-text "1.1.1988"
        key-type "TRAVERSE_TAB_NEXT"
    }
    with [get-editbox -after [get-label Strasse]] {
        set-text "Täfernstrasse 16"
        key-type "TRAVERSE_TAB_NEXT"
    }
    with [get-editbox -after [get-label Plz]] {
        set-text 5405
        key-type "TRAVERSE_TAB_NEXT"
    }
    with [get-editbox -after [get-label Ort]] {
        set-text "Baden-Dättwil"
        key-type "TRAVERSE_TAB_NEXT"
    }
    get-editbox -after [get-label Telefon] | set-text 0815123456
    get-button OK | click
}
