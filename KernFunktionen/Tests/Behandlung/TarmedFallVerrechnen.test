}

if [FallOffen -caseLaw [ t case_KVG ] -startDate "201.*"] {
	log "Offenen Fall KVG gefunden"
} -else {
	FallZuPatientErstellen -caseLaw [ t case_KVG ] -caseReason [ t reason_KVG ] -startDate "20.8.2018"
