Entry-Name: .content

resetPerspective

// To print an AUF we need a text plugin. list of krankenkasse, AUF vorlagen
// The krankenkass comes in via ImportSwissBase

ReadDataImport -file [ concat  [get-user-dir] "/rcptt/Universal_krankenkasse.xls" ]
SelectKontakt -bezeichnung1 "Aquilana"
get-eclipse-window | gen-screenshot [concat $imagesDir "Aquilana_imported.png"]

ConfigureHilotecODF_TextVerarbeitung
ImportHilotecODF_Vorlagen

if [SelectPatient Wu Luis | eq false] {
	PatientenErstellen -familyName Wu -firstName Luis -birthDate "13.04.1980" -street "Vorderstrasse 33" -zipCode 3003 -location Bern -phone "033 323 45 67"
}

if [FallOffen -caseLaw [ t case_UVG ] -startDate "201.*"] {
	log "Offenen Fall UVG gefunden"
} -else {
	FallZuPatientErstellen -caseLaw [ t case_UVG ] -caseReason [ t reason_UVG ] -startDate "20.08.2018"
	log "Fall UVG 20.08.2018 erstellt"
}
if [ HasKonsSelected | eq false] {
	kons-erstellen "BasisPrinting AUF mit UVG"
} -else {
	log "HasKonsSelected returned true"
}

FallErzeugeAUF -percentage 50 -reason "RCP Test Grund" -comment "Und ein noch besserer Kommentar"
ErzeugeBriefZuAUT -reason "RCP Test Grund" 
